{
  "name": "server",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc && yarn create_files && yarn create_static",
    "start": "node dist/index.js",
    "dev": "concurrently \"yarn tsc --watch\" \"yarn create_files && yarn create_static && nodemon -q dist/index.js\"",
    "create_files": "mkdir -p dist/files",
    "create_static": "mkdir -p dist/static"
  },
  "dependencies": {
    "@types/express-fileupload": "^1.4.3",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.2",
    "express-validator": "^6.15.0",
    "express-ws": "^5.0.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  }
}
